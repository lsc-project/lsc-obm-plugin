<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns="http://lsc-project.org/XSD/lsc-obm-plugin-1.0" targetNamespace="http://lsc-project.org/XSD/lsc-obm-plugin-1.0"
	elementFormDefault="qualified" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
	xmlns:lsc="http://lsc-project.org/XSD/lsc-core-2.0.xsd" jaxb:version="2.0">

	<xsd:import namespace="http://lsc-project.org/XSD/lsc-core-2.0.xsd" schemaLocation="lsc-core-2.0.xsd" />

	<xsd:complexType name="obmService" abstract="true">
		<xsd:complexContent>
			<xsd:extension base="lsc:serviceType">
				<xsd:sequence>
					<xsd:element name="domainUUID" type="xsd:string" />
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element name="obmUserService">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="obmService" />
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="obmGroupService">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="obmService" />
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
</xsd:schema>